{
  "name": "hotel-reservation-system",
  "version": "1.0.0",
  "description": "Modern hotel reservation website with role-based access",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "seed": "node scripts/seedDataMultiple.js",
    "export": "node scripts/exportData.js",
    "import": "node scripts/importData.js",
    "migrate": "npm run export && npm run import",
    "build": "cd hotel-react && npm run build",
    "build:prod": "npm run build && npm run start:prod",
    "start:prod": "set NODE_ENV=production && node server.js",
    "check:email": "node checkEmailConfig.js"
  },
  "keywords": [
    "hotel",
    "reservation",
    "booking",
    "nodejs",
    "express",
    "postgresql"
  ],
  "author": "Hotel Management System",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.0.1"
  }
}
